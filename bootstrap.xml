          </management>
          <auto-mac-detect>yes</auto-mac-detect>
        </setting>
      </deviceconfig>
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>
          <zone>
            <entry name="trust">
              <network>
                <virtual-wire>
                  <member>ethernet1/1</member>
                </virtual-wire>
              </network>
            </entry>
